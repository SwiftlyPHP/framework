{
    "home": {
        "path":     "/",
        "methods":  ["GET"],
        "handler": ["App\\Controller\\Home", "index"]
    },
    "hello": {
        "path":     "/hello/[s:name]",
        "methods":  ["GET"],
        "handler":  ["App\\Controller\\Home", "hello"]
    },
    "api.hello": {
        "path":     "/api/hello/[s:name]",
        "handler":  ["App\\Controller\\Api", "hello"]
    }
}
