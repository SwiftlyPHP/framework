{
    "name": "swiftly/framework",
    "description": "A simple PHP framework.",
    "homepage": "https://github.com/SwiftlyPHP/framework",
    "readme": "README.md",
    "type": "project",
    "license": "MIT",
    "require": {
        "swiftly/http": "dev-main",
        "swiftly/config": "dev-main",
        "swiftly/dependency": "dev-main",
        "swiftly/routing": "dev-main",
        "swiftly/database": "dev-main",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "vimeo/psalm": "^4.8",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Swiftly\\Base\\": "src/Swiftly/Base/",
            "Swiftly\\Application\\": "src/Swiftly/Application/",
            "Swiftly\\Middleware\\": "src/Swiftly/Middleware/",
            "Swiftly\\Template\\": "src/Swiftly/Template/",
            "": "app/controller/"
        },
        "files": [
            "src/Swiftly/utilities/functions.php"
        ]
    }
}
